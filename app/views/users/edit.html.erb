<h2>プロフィール編集</h2>
<%= form_with model: @user, url: update_user_path(@user.id), method: :patch do |f| %>
  <label for="imputName">Name</label>
  <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"名前" %>
  <label for="introduction">Introduction</label>
  <%= f.text_field :introduction, autofocus: true, id:"introduction", placeholder:"紹介" %>
  <%= f.submit "Update User" %>
<% end %>
